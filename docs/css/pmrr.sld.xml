<?xml version="1.0" encoding="UTF-8"?>
<StyledLayerDescriptor version="1.0.0" 
  xsi:schemaLocation="http://www.opengis.net/sld StyledLayerDescriptor.xsd" 
  xmlns="http://www.opengis.net/sld" 
  xmlns:ogc="http://www.opengis.net/ogc" 
  xmlns:xlink="http://www.w3.org/1999/xlink" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  
  <NamedLayer>
    <Name>pmrr_mun_wgs</Name>
    <UserStyle>
      <Title>pmrr_mun_wgs</Title>
      <FeatureTypeStyle>
        <Rule>
          <Name>Em andamento</Name>
          <ogc:Filter xmlns:ogc="http://www.opengis.net/ogc">
            <ogc:PropertyIsEqualTo>
              <ogc:PropertyName>status</ogc:PropertyName>
              <ogc:Literal>Em andamento</ogc:Literal>
            </ogc:PropertyIsEqualTo>
          </ogc:Filter>

          <PolygonSymbolizer>
           <Stroke>
             <CssParameter name="stroke">#c51b7d</CssParameter>
             <CssParameter name="stroke-width">2</CssParameter>
           </Stroke>
         </PolygonSymbolizer>


         <PolygonSymbolizer>
          <Fill>
            <GraphicFill>
              <Graphic>
                <Mark>
                  <WellKnownName>shape://slash</WellKnownName>
                  <Stroke>
                    <CssParameter name="stroke">#c51b7d</CssParameter>
                    <CssParameter name="stroke-width">2</CssParameter>
                  </Stroke>
                </Mark>
              </Graphic>
            </GraphicFill>
          </Fill>
        </PolygonSymbolizer>
      </Rule>
      <Rule>
        <Name>Não iniciado</Name>
        <ogc:Filter xmlns:ogc="http://www.opengis.net/ogc">
          <ogc:PropertyIsEqualTo>
            <ogc:PropertyName>status</ogc:PropertyName>
            <ogc:Literal>Não iniciado</ogc:Literal>
          </ogc:PropertyIsEqualTo>
        </ogc:Filter>
        
        <PolygonSymbolizer>
         <Stroke>
           <CssParameter name="stroke">#0053ba</CssParameter>
           <CssParameter name="stroke-width">2</CssParameter>
         </Stroke>
       </PolygonSymbolizer>


       <PolygonSymbolizer>
        <Fill>
          <GraphicFill>
            <Graphic>
              <Mark>
                <WellKnownName>shape://slash</WellKnownName>
                <Stroke>
                  <CssParameter name="stroke">#0053ba</CssParameter>
                  <CssParameter name="stroke-width">2</CssParameter>
                </Stroke>
              </Mark>
            </Graphic>
          </GraphicFill>
        </Fill>
      </PolygonSymbolizer>
    </Rule>
  </FeatureTypeStyle>
</UserStyle>
</NamedLayer>
</StyledLayerDescriptor>
